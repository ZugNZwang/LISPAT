#!/Users/joshuabrummet/workspace/senior-design/LISPAT/lispat_app/lispat/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'scattertext==0.0.2.42','console_scripts','scattertext'
__requires__ = 'scattertext==0.0.2.42'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('scattertext==0.0.2.42', 'console_scripts', 'scattertext')()
    )
